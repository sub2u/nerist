<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>At</th>
    <th>Branch</th>
    <th>Employee</th>
    <th>Student</th>
    <th>Faculty</th>
    <th>Alumini</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.at %></td>
    <td><%= event.branch %></td>
    <td><%= event.employee %></td>
    <td><%= event.student %></td>
    <td><%= event.faculty %></td>
    <td><%= event.alumini %></td>
    <td><%= event.description %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
